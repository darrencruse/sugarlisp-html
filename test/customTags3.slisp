#use "match"
#use "html"

#keyword (function mytag2 {
  case `("mytag2" attributes body)`
    '''<span ~attributes>~body</span>'''

  default (@error "missing argument for mytag");
})

(mytag2 (attr href "http://www.google.com") (tag "div" "hello"))

<mytag2 href="http://www.newtek.com"><div>hello</div></mytag2>

