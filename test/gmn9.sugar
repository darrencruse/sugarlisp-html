#cell a = 0
#cell b = 0

var sum = "?";

sum #= (a, b) => a + b;

console.log("before anything: ${sum}");
a = 1;
console.log("after setting a to 1: ${sum}");
b = 2;
console.log("after setting b to 2: ${sum}");
++a;
console.log("after pre-incrementing a: ${sum}");
--b;
console.log("after pre-decrementing b: ${sum}");
a++;
console.log("after post-incrementing a: ${sum}");
b--;
console.log("after post-decrementing b: ${sum}");
a += 2;
console.log("after a += 2: ${sum}");
b -= 3;
console.log("after b -= 3: ${sum}");
